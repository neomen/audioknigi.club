{
	"manifest_version": 2,
	"name": "Audioknigi.club â€“ Play, Pause",
	"description": "Simple extension for controlling audioknigi.club player",
	"version": "1.0.0",
	"permissions": ["tabs","*://audioknigi.club/*"],
	"icons": {
		"16": "img/icon16.png",
		"32": "img/icon32.png",
		"128": "img/icon.png"
	 },
	"browser_action": {
			"default_icon": "img/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://audioknigi.club/*"
			],
			"js": [
				"libs/jquery.min.js",
				"js/main.js"
			]
		}
	],
	"background": {
			"scripts": ["libs/jquery.min.js","background.js"]
	}
}
